/* pagination.html: https://github.com/spf13/hugo/blob/master/tpl/tplimpl/template_embedded.go#L117 */
.pagination {
  margin: 3rem 0;
}

.pagination li {
  display: inline-block;
  margin-right: .8rem;
  font-size: 1rem;
  margin-bottom: 2.5em;
}

.pagination li a {
  padding: .8rem 1rem;
  background-color: #ededed;
  color: #222;
  border: 1px solid #ddd;
  border-radius: 3px;
  text-decoration: none;
}

.pagination li.disabled {
  display: none;
}

.pagination li.active a:link,
.pagination li.active a:active,
.pagination li.active a:visited {
  background-color: #ddd;
}

#TableOfContents ul li {
  margin-bottom: 1em;
}

.dananke-socials a {
  display: inline-block;
  vertical-align: middle;
  color: #BABABA;
  fill: currentColor;
}

.dananke-socials a .icon {
  display: block;
  line-height: 1;
  padding: .1em;
}

.dananke-socials a .icon svg {
  width: 32px;
  height: 32px;
}

.dananke-socials a {
  padding: 1px;
}

.dananke-socials a:focus, .dananke-socials a:hover {
  border: 1px solid;
  color: #6b7280;
  padding: 0;
}

.new-window {
  opacity: 0;
  display: inline-block;
  vertical-align: top;
}

.link-transition:hover .new-window {
  opacity: 1;
}

#TableOfContents li ul {
  margin-top: 1em;
  padding-left: 1em;
}

.presentation-pdf div {
  overflow: auto;
  resize: both;
}

.presentation-pdf div object {
  border: 2px solid;
  display: block;
  height: 760px;
  margin-right: .8em;
  resize: both;
  width: 100%;
}

@media (max-height: 499px) {
  .presentation-pdf div object {
    height: 100vh;
  }
}

@media (max-height: 900px) {
  .presentation-pdf div object {
    height: 480px;
  }
}

.intro-card-pdf-gallery {
  display: inline-block;
  margin-left: .8em;
  margin-right: .8em;
  margin-top: .3em;
  text-align: center;
  vertical-align: baseline;
}

.intro-card-pdf-gallery div {
  height: 665px;
  overflow: auto;
  text-align: center;
  width: 700px;
}

.intro-card-pdf-gallery object {
  border: 2px solid;
  display: block;
  height: 100%;
  margin-right: .8em;
  width: 100%;
}

main table {
  border-collapse: collapse;
  width: 100%;
}

main table tr th {
  background-color: silver;
  border-bottom: 4px ridge;
  border-left: 4px ridge;
  border-top: 4px ridge;
  text-align: left;
  width: max-content;
}

@supports (text-align: start) {
  main table tr th {
    text-align: start;
  }
}

main table tr th:last-of-type {
  border-right: 4px ridge;
}

main table tr td {
  border-bottom: 2px solid;
  border-left: 2px solid;
  width: max-content;
}

main table tr td:last-of-type {
  border-right: 2px solid;
  min-width: 50%;
}

main table tbody tr:nth-of-type(even) {
  background-color: lightgray;
}

